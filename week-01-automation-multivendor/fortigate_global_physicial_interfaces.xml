  <global xmlns="http://tail-f.com/ned/fortinet-fortios">
    <system>
      <interface>
        <interface-list>
          <name>port3</name>

          <allowaccess>https</allowaccess>
          <allowaccess>ping</allowaccess>
          <allowaccess>ssh</allowaccess>

          <status>up</status>
          <vdom>root</vdom>
          <type>physical</type>

          <ip>
